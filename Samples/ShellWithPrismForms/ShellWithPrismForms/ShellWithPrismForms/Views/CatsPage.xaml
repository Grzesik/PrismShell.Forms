<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ShellWithPrismForms.Views.CatsPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="CATS"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />

            <Button Text="Call page" Command="{Binding CallPage}" Margin="20" VerticalOptions="Start" HorizontalOptions="Center"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>